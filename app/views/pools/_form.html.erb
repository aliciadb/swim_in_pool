
  <%= simple_form_for @pool do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :location %>
    <%= f.input :category, collection: Pool::CATEGORIES %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit, class: 'btn btn-light' %>
  <% end %>
